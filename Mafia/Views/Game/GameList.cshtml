@{
    @model List<PresentationLayer.Models.GameViewModel>
}

<div class="alert-heading">
    @{
        if (Model != null)
        {
            foreach (var game in Model)
            {
                <a asp-action="Index" asp-route-gameId="@game.Game.Id"><h1>@game.Game.Id</h1></a>
                <h1>@game.Game.Date</h1>
            }
        }
    }
    
</div>
